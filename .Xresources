!!!!!!! URxvt settings !!!!!!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
URxvt.locale:           true
URxvt.saveLines:        8000
URxvt.perl-ext-common:  default,matcher,resize-font,clipboard,selection-to-clipboard
URxvt.scrollstyle:      plain
URxvt.secondaryScreen: 1
URxvt.secondaryScroll:  0
URxvt.secondaryWheel: 1

URxvt.url-launcher:     /usr/bin/xdg-open
URxvt.matcher.button:   1

URxvt.font: xft:Inconsolata:size=12:antialias=true
URxvt.keysym.C-Down:    resize-font:smaller
URxvt.keysym.C-Up:      resize-font:bigger

URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:pase

URxvt.depth:            24
URxvt.geometry:         100x40
URxvt*internalBorder:   5
URxvt*externalBorder:   0
URxvt*scrollBar:        false
URxvt*scrollBar_right:  false

URxvt.visualBell: true
URxvt.iso14755: false

*background:   [97]#282a36
*foreground:   #eff0eb
*cursorColor:  #9aedfe

*color0:      #33303b
*color8:      #4f4b58
*color1:      #ff5c57
*color9:      #ff5c57
*color2:      #5af78e
*color10:     #5af78e
*color3:      #f3f99d
*color11:     #f3f99d
*color4:      #57c7ff
*color12:     #57c7ff
*color5:      #ff6ac1
*color13:     #ff6ac1
*color6:      #9aedfe
*color14:     #9aedfe
*color7:      #eff0eb
